<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
    xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:s="library://ns.adobe.com/flex/spark">
	
    <fx:Metadata>
        [HostComponent("spark.components.Button")]
    </fx:Metadata>

    <s:states>
        <s:State name="up"/>
        <s:State name="over"/>
        <s:State name="down"/>
        <s:State name="disabled"/>
        <s:State name="upAndSelected"/>
        <s:State name="overAndSelected"/>
        <s:State name="downAndSelected"/>
        <s:State name="disabledAndSelected"/>
    </s:states>
	
	<s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_15.png')" includeIn="up" />
	<s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_15.png')" includeIn="disabled" alpha="0.5" />
    <s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_15.png')" includeIn="over" />
    <s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_15.png')" includeIn="down" alpha="0.8" />
	
	<s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_25.png')" includeIn="upAndSelected" />
	<s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_25.png')" includeIn="disabledAndSelected" alpha="0.5" />
    <s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_25.png')" includeIn="overAndSelected" />
    <s:BitmapImage smooth="true" source="@Embed(source='assets/todo/ToDo2_25.png')" includeIn="downAndSelected" alpha="0.8" />
	<s:Label id="labelDisplay" />
	
</s:SparkSkin>