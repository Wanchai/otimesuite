<?xml version="1.0" encoding="utf-8"?>
<s:Skin
    xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:s="library://ns.adobe.com/flex/spark">
	
    <fx:Metadata>
        [HostComponent("com.odelay.SkinnableAlert")]
    </fx:Metadata>
	
	<s:Rect top="0" left="0" right="0" bottom="0">
		<s:fill>
			<s:SolidColor color="0x000000" />
		</s:fill>
	</s:Rect>
		
	<s:VGroup horizontalAlign="right" paddingTop="10" paddingBottom="10" paddingLeft="10" paddingRight="10">
		
		<s:Button id="closeButton" skinClass="skins.btn.CloseTitleWindowSkin" />
		
		<s:VGroup horizontalAlign="center">
			<s:TextArea id="txt" lineHeight="3" editable="false" selectable="false" borderVisible="false" contentBackgroundAlpha="0"
				textAlign="center" styleName="textNormWhite" />
			<s:Button id="buttonOne" skinClass="skins.btn.DefaultBtn" />
		</s:VGroup>
		
	</s:VGroup>
	
</s:Skin>